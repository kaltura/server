define command{
        command_name check_api_v3_log
        command_line $USER1$/check_log -F /opt/kaltura/log/kaltura_api_v3.log -O /opt/kaltura/tmp/kaltura_api_v3.log -q "ERR:|PHP|trace|CRIT|\[error\]"
}

define command{
        command_name check_kaltura_apache_log
        command_line $USER1$/check_log -F /opt/kaltura/log/kaltura_apache_errors.log -O /opt/kaltura/tmp/kaltura_apache_errors.log -q "PHP|\[error\]"
}


# $USER6$ should be defined in resources.cfg and contain the 'kaltura' DB name, $USER7$ holds the kaltura DB user which is 'kaltura' by default and $USER8$ should hold the passwd
define command{
        command_name check_kaltura_mysql
        command_line  $USER1$/check_mysql -H $HOSTADDRESS$ -d $USER6$ -u$USER7$ -p$USER8$ -w10 -c20
}

# $USER9$ should be defined in resources.cfg and contain the 'kalturadw' DB name, $USER10$ holds the kaltura DB user which is 'etl' by default and $USER8$ should hold the passwd
define command{
        command_name check_kaltura_dwh_mysql
        command_line  $USER1$/check_mysql -H $HOSTADDRESS$ -d $USER6$ -u$USER7$ -p$USER8$ -w10 -c20
}

define command{
        command_name check_ntp_time
        command_line $USER1$/check_ntp_time -H $HOSTADDRESS$ -w 2 -c 5
}

