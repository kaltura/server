{
  "settings" : {
	"number_of_shards" : 6,
	"number_of_replicas" : 0,
	"analysis": {
	  "filter":{
		"english_stop": {
		  "type":       "stop",
		  "stopwords":  "_english_"
		},
		"english_stemmer": {
		  "type":       "stemmer",
		  "language":   "english"
		},
		"english_possessive_stemmer": {
		  "type":       "stemmer",
		  "language":   "possessive_english"
		},
		"kaltura_english_synonym_filter": {
		  "type": "synonym",
		  "format": "wordnet",
		  "synonyms_path": "analysis/wn_s.pl"
		},
		"kaltura_unique_token_filter": {
		  "type": "unique",
		  "only_on_same_position": true
		}
	  },
	  "char_filter": {
		"kaltura_whitespace_filter": {
		  "type": "pattern_replace",
		  "pattern": "(\\s+)",
		  "replacement": " "
		}
	  },
	  "tokenizer": {
		"kaltura_ngram_tokenizer": {
		  "type": "ngram",
		  "min_gram": 3,
		  "max_gram": 3,
		  "token_chars": [
			"letter",
			"digit"
		  ]
		},
		"kaltura_category_name_tokenizer" : {
		  "type": "pattern",
		  "pattern": ">"
		}
	  },
	  "analyzer": {
		"kaltura_ngrams": {
		  "type": "custom",
		  "tokenizer": "kaltura_ngram_tokenizer",
		  "filter": [
			"lowercase"
		  ]
		},
		"kaltura_keyword": {
		  "type": "custom",
		  "tokenizer": "keyword",
		  "char_filter": [
			"kaltura_whitespace_filter"
			],
		  "filter": [
			"lowercase",
			"trim",
			"asciifolding"
		  ]
		},
		"kaltura_text" : {
		  "tokenizer" : "standard",
		  "filter" : ["standard", "lowercase", "asciifolding"]
		},
		"kaltura_english_synonym": {
		  "tokenizer":  "standard",
		  "filter": [
			"english_possessive_stemmer",
			"lowercase",
			"english_stop",
			"english_stemmer",
			"kaltura_english_synonym_filter",
			"kaltura_unique_token_filter"
		  ]
		},
		"kaltura_category_name_analyzer": {
		  "tokenizer": "kaltura_category_name_tokenizer",
		  "char_filter": [
			"kaltura_whitespace_filter"
		  ],
		  "filter": [
			"lowercase",
			"trim",
			"asciifolding"
		  ]
		}
	  }
	}
  },
