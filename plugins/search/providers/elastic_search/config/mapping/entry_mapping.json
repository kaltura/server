{
  "settings" : {
	"number_of_shards" : 12,
	"number_of_replicas" : 0,
	"analysis": {
	  "filter":{
		"english_stop": {
		  "type":       "stop",
		  "stopwords":  "_english_"
		},
		"english_stemmer": {
		  "type":       "stemmer",
		  "language":   "english"
		},
		"english_possessive_stemmer": {
		  "type":       "stemmer",
		  "language":   "possessive_english"
		},
		"kaltura_english_synonym_filter": {
		  "type": "synonym",
		  "format": "wordnet",
		  "synonyms_path": "analysis/wn_s.pl"
		},
		"kaltura_unique_token_filter": {
		  "type": "unique",
		  "only_on_same_position": true
		}
	  },
	  "char_filter": {
		"kaltura_whitespace_filter": {
		  "type": "pattern_replace",
		  "pattern": "(\\s+)",
		  "replacement": " "
		},
		"kaltura_ngram_special_chars_filter": {
		  "type": "mapping",
		  "mappings": [
			"'=>",
			"`=>",
			"â€˜=>",
			"^=>",
			"~=>"
		  ]
		},
		"kaltura_special_chars_filter": {
		  "type": "mapping",
		  "mappings": [
			"!=>__exclamationmark__",
			"@=>__at__",
			"#=>__number__",
			"$=>__dollar__",
			"%=>__percent__",
			"^=>__caret__",
			"&=>__ampersand__",
			"*=>__asterisk__",
			"(=>__openbracket__",
			")=>__closebracket__",
			"-=>__hyphen__",
			"==>__equals__",
			"+=>__plus__",
			"?=>__questionmark__",
			"/=>__slash__",
			"\\\\=>__backslash__",
			"[=>__openbracket1__",
			"]=>__closebracket1__",
			"{=>__openbracket2__",
			"}=>__closebracket2__",
			";=>__semicolon__",
			"|=>__verticalbar__",
			"<=>__lessthan__",
			">=>__greaterthan__",
			"~=>__tilde__"
		  ]
		}
	  },
	  "tokenizer": {
		"kaltura_ngram_tokenizer": {
		  "type": "ngram",
		  "min_gram": 3,
		  "max_gram": 3,
		  "token_chars": [
			"letter",
			"digit"
		  ]
		}
	  },
	  "normalizer": {
		"kaltura_keyword_normalizer": {
		  "type": "custom",
		  "char_filter": [
			"kaltura_whitespace_filter"
		  ],
		  "filter": [
			"lowercase",
			"icu_folding"
		  ]
		}
	  },
	  "analyzer": {
		"kaltura_ngrams": {
		  "type": "custom",
		  "tokenizer": "kaltura_ngram_tokenizer",
		  "char_filter": [
			"kaltura_ngram_special_chars_filter"
		  ],
		  "filter": [
			"lowercase",
			"icu_folding"
		  ]
		},
		"kaltura_text" : {
		  "type": "custom",
		  "tokenizer": "standard",
		  "char_filter": [
			"kaltura_special_chars_filter"
		  ],
		  "filter": [
			"standard",
			"lowercase",
			"icu_folding"
		  ]
		},
		"kaltura_english_synonym": {
		  "tokenizer":  "standard",
		  "filter": [
			"lowercase",
			"english_stop",
			"kaltura_english_synonym_filter",
			"kaltura_unique_token_filter"
		  ]
		}
	  }
	}
