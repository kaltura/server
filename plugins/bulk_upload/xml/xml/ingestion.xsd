				<example>
					<item>
						<action>add</action>
						<type>1</type>
						<media>...</media>
						<contentAssets>
							<content>
								<streams>...</streams>
							</content>
						</contentAssets>
					</item>
				</example>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
