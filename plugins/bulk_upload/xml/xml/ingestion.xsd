				<example>
					<item>
						<action>add</action>
						<type>1</type>
						<media>...</media>
						<contentAssets>
							<content>
								<streams>
									<stream type="audio" trackIndex="1" language="eng" label="something"/>
								</streams>
							</content>
						</contentAssets>
					</item>
				</example>
			</xs:appinfo>
