	Not supported yet

	<xs:complexType name="T_playlist">
		<xs:sequence>
			<xs:element name="playlistType" minOccurs="1" maxOccurs="1" type="KalturaPlaylistType"/>
			<xs:element name="playlistContent" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>
						Content of the playlist - 
							- XML if the playlistType is dynamic 
							- Text if the playlistType is static 
							- URL if the playlistType is mRss 
					</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:element ref="playlist-extension" minOccurs="0" maxOccurs="unbounded" />

		</xs:sequence>
	</xs:complexType>
	-->

	<!--
	 
	Not supported yet

	<xs:complexType name="T_data">
		<xs:sequence>
			<xs:element name="dataContent" minOccurs="0" maxOccurs="1" type="xs:string"/>
			<xs:element name="retrieveDataContentByGet" minOccurs="0" maxOccurs="1" type="xs:boolean" />

			<xs:element ref="data-extension" minOccurs="0" maxOccurs="unbounded" />

		</xs:sequence>
	</xs:complexType>
	-->

	<!--
	 
	Not supported yet

	<xs:complexType name="T_liveStream">
		<xs:complexContent>
			<xs:extension base="T_media">
				<xs:sequence>
					<xs:element ref="bitrates" minOccurs="0" maxOccurs="1"/>
					<xs:element name="encodingIP1" minOccurs="1" maxOccurs="1" />
					<xs:element name="encodingIP2" minOccurs="0" maxOccurs="1" />
					<xs:element name="streamPassword" minOccurs="0" maxOccurs="1" />

					<xs:element ref="liveStream-extension" minOccurs="0" maxOccurs="unbounded" />

				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
