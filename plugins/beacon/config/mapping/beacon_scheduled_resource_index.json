{
	"settings": {
		"index": {
			"search": {
				"slowlog": {
					"threshold": {
						"fetch": {
							"warn": "10s",
							"trace": "500ms",
							"debug": "2s",
							"info": "5s"
						},
						"query": {
							"warn": "10s",
							"trace": "500ms",
							"debug": "2s",
							"info": "5s"
						}
					}
				}
			},
			"indexing": {
				"slowlog": {
					"threshold": {
						"index": {
							"warn": "10s",
							"trace": "500ms",
							"debug": "2s",
							"info": "5s"
						}
					}
				}
			},
			"mapping": {
				"total_fields": {
					"limit": "2000"
				}
			}
		},
		"number_of_shards": 6,
		"number_of_replicas": 1,
		"analysis": {
			"char_filter": {
				"kaltura_whitespace_filter": {
					"type": "pattern_replace",
					"pattern": "(\\s+)",
					"replacement": " "
				}
			},
			"tokenizer": {
				"kaltura_ngram_tokenizer": {
					"type": "ngram",
					"min_gram": 3,
					"max_gram": 3,
					"token_chars": [
						"letter",
						"digit"
					]
				}
			},
			"normalizer": {
				"kaltura_keyword_normalizer": {
					"type": "custom",
					"char_filter": [
						"kaltura_whitespace_filter"
					],
					"filter": [
						"lowercase"
					]
				}
			},
			"analyzer": {
				"kaltura_ngrams": {
					"type": "custom",
					"tokenizer": "kaltura_ngram_tokenizer",
					"filter": [
						"lowercase"
					]
				},
				"kaltura_text": {
					"type": "custom",
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase"
					]
				},
				"kaltura_text_partial_search" : {
					"type": "custom",
					"tokenizer": "standard",
					"filter": [
						"standard",
						"lowercase"
					]
				}
			}
		}
	},
	"mappings": {
		"scheduled_resource_beacon": {
			"dynamic_templates": [{
				"integers": {
					"match_mapping_type": "long",
					"mapping": {
						"type": "float"
					}
				}}
			,
				{
					"strings": {
						"match_mapping_type": "string",
						"mapping": {
							"type": "keyword",
							"normalizer": "kaltura_keyword_normalizer"
						}
					}
				}
			],
			"_all": {
				"enabled": false
			},
			"properties": {
				"event_type": {
					"type": "keyword",
					"normalizer": "kaltura_keyword_normalizer"
				},
				"object_id": {
					"type": "keyword",
					"normalizer": "kaltura_keyword_normalizer"
				},
				"partner_id": {
					"type": "keyword",
					"normalizer": "kaltura_keyword_normalizer"
				},
				"private_data": {
					"properties": {
						"Errors": {
							"properties": {
								"APP_STATUS": {
									"type": "keyword",
									"normalizer": "kaltura_keyword_normalizer"
								},
								"DEVICE_ERROR": {
									"properties": {
										"AUDIO": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"SCREEN": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"VIDEO": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										}
									}
								},
								"RECORDING_ERROR": {
									"properties": {
										"CANCEL_RECORDING": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"PAUSE_RECORDING": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"RECOVER_RECORDING": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"RESUME_RECORDING": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"START_RECORDING": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										},
										"STOP_RECORDING": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										}
									}
								},
								"SYS_HEALTH_ERROR": {
									"properties": {
										"DISKSPACE": {
											"properties": {
												"text": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"time": {
													"type": "float"
												}
											}
										}
									}
								}
							}
						},
						"RecordingData": {
							"properties": {
								"recordingPhase": {
									"type": "keyword",
									"normalizer": "kaltura_keyword_normalizer"
								},
								"recordingTime": {
									"type": "keyword",
									"normalizer": "kaltura_keyword_normalizer"
								},
								"isScheduledEvent": {
									"type": "boolean"
								},
								"streamingInputObjects": {
									"properties": {
										"primaryInput": {
											"properties": {
												"templateEntryId": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"status": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"duration": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												},
												"updatedAt": {
													"type": "keyword",
													"normalizer": "kaltura_keyword_normalizer"
												}
											}
										}
									}
								}
							}
						},
						"ResourceDetails": {
							"properties": {
								"activeSessions": {
									"properties": {
										"primary": {
											"type": "boolean"
										},
										"secondary": {
											"type": "boolean"
										}
									}
								},
								"previewingSessions": {
									"properties": {
										"primary": {
											"type": "boolean"
										},
										"secondary": {
											"type": "boolean"
										}
									}
								},
								"appVersion": {
									"type": "keyword",
									"normalizer": "kaltura_keyword_normalizer"
								},
								"resourceId": {
									"type": "float"
								},
								"resourceName": {
									"type": "text",
									"analyzer": "kaltura_text",
									"fields": {
										"ngrams": {
											"type": "text",
											"analyzer": "kaltura_ngrams"
										},
										"raw": {
											"type": "keyword",
											"normalizer": "kaltura_keyword_normalizer"
										}
									}
								}
							}
						},
						"Scheduling": {
							"properties": {
								"NextScheduledEvent": {
									"properties": {
										"description": {
											"type": "keyword",
											"normalizer": "kaltura_keyword_normalizer"
										},
										"endDate": {
											"type": "float"
										},
										"id": {
											"type": "float"
										},
										"organizer": {
											"type": "keyword",
											"normalizer": "kaltura_keyword_normalizer"
										},
										"startDate": {
											"type": "float"
										},
										"summary": {
											"type": "keyword",
											"normalizer": "kaltura_keyword_normalizer"
										}
									}
								},
								"currentScheduledEvent": {
									"properties": {
										"id": {
											"type": "float"
										},
										"summary": {
											"type": "keyword",
											"normalizer": "kaltura_keyword_normalizer"
										},
										"isLiveEvent": {
											"type": "boolean"
										},
										"templateEntryId": {
											"type": "keyword",
											"normalizer": "kaltura_keyword_normalizer"
										}
									}
								}
							}
						},
						"appVersion": {
							"type": "keyword",
							"normalizer": "kaltura_keyword_normalizer"
						},
						"status": {
							"type": "keyword",
							"normalizer": "kaltura_keyword_normalizer"
						},
						"uploadQueue": {
							"type": "float"
						},
						"uploadingCount": {
							"type": "float"
						}
					}
				},
				"raw_data": {
					"type": "keyword",
					"index": "false"
				},
				"related_object_type": {
					"type": "keyword",
					"normalizer": "kaltura_keyword_normalizer"
				},
				"updated_at": {
					"type": "date",
					"format": "epoch_second"
				},
				"is_log": {
					"type": "boolean"
				}
			}
		}
	}
}