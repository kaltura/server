<?xml version="1.0" encoding="UTF-8"?>
<xml>
	<config>
		<serviceUrl>@SERVICE_URL@</serviceUrl>
		<partnerId>99</partnerId>
		<clientTag>Stand alone php 1.0.0</clientTag>
		<curlTimeout>120</curlTimeout>
		<userAgent>Stand alone php 1.0.0</userAgent>
		<verifySSL objectType="bool">0</verifySSL>
	</config>
	<session>
		<partnerId>99</partnerId>
		<secret>@TEMPLATE_PARTNER_ADMIN_SECRET@</secret>
		<userId></userId>
		<sessionType>2</sessionType>
		<expiry>86400</expiry>
		<privileges></privileges> 
	</session>
	<request service="accessControlProfile" action="add">
		<accessControlProfile objectType="KalturaAccessControlProfile">
			<name>Default</name>
			<systemName>Default</systemName>
			<description>Default access control profile</description>
			<isDefault>1</isDefault>
		</accessControlProfile>
	</request>
</xml>