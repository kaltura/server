<?xml version="1.0" encoding="UTF-8"?>
<xml>
    <config>
        <serviceUrl>@SERVICE_URL@</serviceUrl>
        <partnerId>-2</partnerId>
        <clientTag>Stand alone php 1.0.0</clientTag>
        <curlTimeout>30</curlTimeout>
        <userAgent>Stand alone php 1.0.0</userAgent>
    </config>
    <session>
        <partnerId>-2</partnerId>
        <secret>@ADMIN_CONSOLE_PARTNER_ADMIN_SECRET@</secret>
        <sessionType>2</sessionType>
    </session>

    <multirequest>
        <request service="responseProfile" action="listAction" partnerId="0">
            <filter objectType="KalturaResponseProfileFilter">
                <systemNameEqual>reach_vendor</systemNameEqual>
            </filter>
            <pager objectType="KalturaFilterPager">
                <pageSize>1</pageSize>
            </pager>
        </request>
        <request service="responseProfile" action="update" partnerId="0">
            <id>{1:result:objects:0:id}</id>
            <responseProfile objectType="KalturaResponseProfile">
                <type>1</type>
                <name>reach_vendor</name>
                <systemName>reach_vendor</systemName>
                <fields>id,partnerId,vendorPartnerId,createdAt,entryId,status,version,reachProfileId,catalogItemId,accessKey,notes,dictionary,taskJobData,captionAssetId,jsonTranscriptAssetId,textTranscriptAssetId,price,externalTaskId </fields>
            </responseProfile>
        </request>
    </multirequest>
</xml>