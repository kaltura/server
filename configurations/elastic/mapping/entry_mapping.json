{
	"settings" : {
		"index" : {
			"search" : {
				"slowlog" : {
					"threshold" : {
						"fetch": {
							"warn": "10s",
							"trace": "500ms",
							"debug": "2s",
							"info": "5s"
						},
						"query": {
							"warn": "10s",
							"trace": "500ms",
							"debug": "2s",
							"info": "5s"
						}
					}
				}
			},
			"indexing" : {
				"slowlog" : {
					"threshold" : {
						"index": {
							"warn": "10s",
							"trace": "500ms",
							"debug": "2s",
							"info": "5s"
						}
					}
				}
			}
		},
		"number_of_shards" : 12,
		"number_of_replicas" : 0,
		"analysis": {
			"filter":{
				"english_stop": {
					"type":       "stop",
					"stopwords":  "_english_"
				},
				"english_stemmer": {
					"type":       "stemmer",
					"language":   "english"
				},
				"english_possessive_stemmer": {
					"type":       "stemmer",
					"language":   "possessive_english"
				},
				"kaltura_english_synonym_filter": {
					"type": "synonym",
					"synonyms_path": "analysis/kaltura_synonyms_contraction.txt"
				},
				"kaltura_unique_token_filter": {
					"type": "unique",
					"only_on_same_position": true
				}
			},
			"char_filter": {
				"kaltura_whitespace_filter": {
					"type": "pattern_replace",
					"pattern": "(\\s+)",
					"replacement": " "
				},
				"kaltura_apostrophe_filter": {
					"type": "mapping",
					"mappings": [
						"'=>",
						"`=>",
						"‘=>"
					]
				},
				"kaltura_special_chars_filter": {
					"type": "mapping",
					"mappings": [
						"!=>__exclamationmark__",
						"@=>__at__",
						"#=>__number__",
						"$=>__dollar__",
						"%=>__percent__",
						"^=>__caret__",
						"&=>__ampersand__",
						"*=>__asterisk__",
						"(=>__openbracket__",
						")=>__closebracket__",
						"-=>__hyphen__",
						"==>__equals__",
						"+=>__plus__",
						"?=>__questionmark__",
						"/=>__slash__",
						"\\\\=>__backslash__",
						"[=>__openbracket1__",
						"]=>__closebracket1__",
						"{=>__openbracket2__",
						"}=>__closebracket2__",
						";=>__semicolon__",
						"|=>__verticalbar__",
						"<=>__lessthan__",
						">=>__greaterthan__",
						"~=>__tilde__"
					]
				},
				"kaltura_remove_special_chars_filter": {
					"type": "mapping",
					"mappings": [
						"'=>",
						"`=>",
						"‘=>",
						"!=>",
						"@=>",
						"#=>",
						"$=>",
						"%=>",
						"^=>",
						"&=>",
						"*=>",
						"(=>",
						")=>",
						"-=>",
						"==>",
						"+=>",
						"?=>",
						"/=>",
						"\\\\=>",
						"[=>",
						"]=>",
						"{=>",
						"}=>",
						";=>",
						"|=>",
						"<=>",
						">=>",
						"~=>"
					]
				}
			},
			"tokenizer": {
				"kaltura_ngram_tokenizer": {
					"type": "ngram",
					"min_gram": 3,
					"max_gram": 3,
					"token_chars": [
						"letter",
						"digit"
					]
				}
			},
			"normalizer": {
				"kaltura_keyword_normalizer": {
					"type": "custom",
					"char_filter": [
						"kaltura_whitespace_filter"
					],
					"filter": [
						"lowercase",
						"icu_folding"
					]
				}
			},
			"analyzer": {
				"kaltura_ngrams": {
					"type": "custom",
					"tokenizer": "kaltura_ngram_tokenizer",
					"char_filter": [
						"kaltura_apostrophe_filter"
					],
					"filter": [
						"lowercase",
						"icu_folding"
					]
				},
				"kaltura_text" : {
					"type": "custom",
					"tokenizer": "standard",
					"char_filter": [
						"kaltura_apostrophe_filter",
						"kaltura_special_chars_filter"
					],
					"filter": [
						"standard",
						"lowercase",
						"icu_folding"
					]
				},
				"kaltura_text_partial_search" : {
					"type": "custom",
					"tokenizer": "standard",
					"char_filter": [
						"kaltura_apostrophe_filter"
					],
					"filter": [
						"standard",
						"lowercase",
						"icu_folding"
					]
				},
				"kaltura_english_synonym": {
					"tokenizer":  "whitespace",
					"filter": [
						"lowercase",
						"english_stop",
						"kaltura_english_synonym_filter",
						"kaltura_unique_token_filter"
					]
				},
				"kaltura_english_synonym_search": {
					"tokenizer":  "whitespace",
					"char_filter": [
						"kaltura_remove_special_chars_filter"
					],
					"filter": [
						"lowercase",
						"english_stop",
						"kaltura_english_synonym_filter"
					]
				}
			}
		}
	},
