	<xs:element name="content" type="T_content">
		<xs:annotation>
			<xs:documentation>Flavor asset element</xs:documentation>
			<xs:appinfo>
				<example>
					<item>
						<type>1</type>
						<media>
							<mediaType>1</mediaType>
						</media>
						<content	url="http://kaltura.domain/path/my_flavor_asset.flv"
									isSource="true" 
									containerFormat="flash video" 
									extension="flv" 
									assetId="{flavor asset id}"
									flavorParamsId="0"
									flavorParamsName="Source" 
									format="" 
									videoBitrate="0" 
									videoCodec="" 
									audioBitrate="0" 
									audioCodec="" 
									frameRate="0" 
									height="0" 
									width="0"
						>
							<tags>
								<tag>example</tag>
								<tag>my_tag</tag>
							</tags>
						</content>
					</item>
				</example>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="thumbnail" type="T_thumbnail">
		<xs:annotation>
			<xs:documentation>Thumbnail asset element</xs:documentation>
			<xs:appinfo>
				<example>
					<item>
						<type>1</type>
						<media>
							<mediaType>1</mediaType>
						</media>
						<thumbnail	url="http://kaltura.domain/path/my_thumbnail_asset.flv" 
									thumbAssetId="{thumb asset id}" 
									isDefault="false" 
									format="" 
									height="120" 
									width="120"
						>
							<tags>
								<tag>example</tag>
								<tag>my_tag</tag>
							</tags>
						</thumbnail>
					</item>
				</example>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="player" type="T_player">
		<xs:annotation>
			<xs:documentation>Entry player element</xs:documentation>
			<xs:appinfo>
				<example>
					<item>
						<type>1</type>
						<media>
							<mediaType>1</mediaType>
						</media>
						<player	url="http://kaltura.domain/path/my_player_widget.swf"/>
					</item>
				</example>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
